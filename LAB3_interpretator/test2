<PROGRAM>

<VARDECLARATION><VAR = y><TYPE>INT</TYPE><DIMENSIONS count = 1><DIMENSION>1</DIMENSION></DIMENSIONS><VALUES><VALUE>1</VALUE></VALUES></VAR></VARDECLARATION>
<VARDECLARATION><VAR = x><TYPE>INT</TYPE><DIMENSIONS count = 1><DIMENSION>1</DIMENSION></DIMENSIONS><VALUES><VALUE>4</VALUE></VALUES></VAR></VARDECLARATION>

<FUNC name = assign>

<VARDECLARATION><VAR = b CONST = false><TYPE>BOOL</TYPE><DIMENSIONS count = 2><DIMENSION>5</DIMENSION><DIMENSION>5</DIMENSION></DIMENSIONS><VALUES><VALUE>1</VALUE><VALUE>1</VALUE><VALUE>1</VALUE><VALUE>1</VALUE><VALUE>1</VALUE><VALUE>1</VALUE><VALUE>1</VALUE><VALUE>1</VALUE><VALUE>1</VALUE><VALUE>1</VALUE><VALUE>1</VALUE><VALUE>1</VALUE><VALUE>1</VALUE><VALUE>1</VALUE><VALUE>1</VALUE><VALUE>1</VALUE><VALUE>1</VALUE><VALUE>1</VALUE><VALUE>1</VALUE><VALUE>1</VALUE><VALUE>1</VALUE><VALUE>1</VALUE><VALUE>1</VALUE><VALUE>1</VALUE><VALUE>1</VALUE></VALUES></VAR></VARDECLARATION>

<SWITCH><CONDITION><CHECK><EQ><VAR name = b><DIM><INDEX><VAR name = x></VAR></INDEX><INDEX><VAR name = y></VAR></INDEX></DIM></VAR> 1</EQ></CHECK>
<DO><ASSIGN><VALUE>0</VALUE><TO><VAR name = b><DIM><INDEX><VAR name = x></VAR></INDEX><INDEX><VAR name = y></VAR></INDEX></DIM></VAR></TO></ASSIGN></DO>
</CONDITION></SWITCH>


</FUNC>

<FUNC name = main>
<LEFT>1</LEFT>
<RIGHT>1</RIGHT>
<LEFT>2</LEFT>
<VARDECLARATION><VAR = a><TYPE>CELL</TYPE><DIMENSIONS count = 1><DIMENSION>1</DIMENSION></DIMENSIONS><VALUES><VALUE>WALL</VALUE></VALUES></VAR>
<VAR = c CONST = false><TYPE>CELL</TYPE></VAR>
</VARDECLARATION>


<ASSIGN><VALUE>UNDEF</VALUE><TO><VAR name = a></VAR><VAR name = c></VAR></TO></ASSIGN>

<ASSIGN><VALUE><ADD><VAR name = y></VAR> 1</ADD></VALUE><TO><VAR name = y></VAR></TO></ASSIGN>

<VARDECLARATION><VAR = i><TYPE>INT</TYPE><DIMENSIONS count   = 1><DIMENSION>1</DIMENSION></DIMENSIONS><VALUES><VALUE>4</VALUE></VALUES></VAR>
</VARDECLARATION>

<SWITCH><CONDITION><CHECK><NOT><EQ><VAR name = y></VAR> 5</EQ></NOT></CHECK>
<DO><CALL>main</CALL></DO></CONDITION></SWITCH>

<WHILE><CHECK><NOT><EQ><VAR name = i></VAR> 5</EQ></NOT></CHECK>
<DO><ASSIGN><VALUE><ADD><VAR name = i></VAR> 1</ADD></VALUE><TO><VAR name = i></VAR></TO></ASSIGN></DO></WHILE>

<SWITCH><CONDITION><CHECK><NOT><EQ><VAR name = i></VAR> 50000</EQ></NOT></CHECK>
<DO><ASSIGN><VALUE><ADD><VAR name = i></VAR> 1</ADD></VALUE><TO><VAR name = i></VAR></TO></ASSIGN></DO>
</CONDITION><CONDITION><CHECK><NOT><EQ><VAR name = i></VAR> 5</EQ></NOT></CHECK>
<DO><ASSIGN><VALUE><ADD><VAR name = i></VAR> 1</ADD></VALUE><TO><VAR name = i></VAR></TO></ASSIGN></DO>
</CONDITION></SWITCH>

</FUNC>

</PROGRAM>

